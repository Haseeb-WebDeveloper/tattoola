{
  "expo": {
    "name": "tattoola",
    "slug": "tattoola",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "orientation": "portrait",
    "icon": "./src/assets/app-icons/favicon.png",
    "scheme": "tattoola",
    "userInterfaceStyle": "dark",
    "platforms": ["ios", "android"],
    "newArchEnabled": true,
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/624fc895-19a3-4765-9329-1f3ecb47aa61"
    },
    "ios": {
      "buildNumber": "11",
      "supportsTablet": true,
      "bundleIdentifier": "com.figmentalabs.tattoola",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "L'app ha bisogno di accedere alle tue foto per caricare la foto del profilo e i tatuaggi.",
        "NSCameraUsageDescription": "L'app ha bisogno di accedere alla fotocamera per scattare foto.",
        "NSPhotoLibraryAddUsageDescription": "L'app ha bisogno di salvare foto nella tua galleria."
      },
      "associatedDomains": ["applinks:haehelhifvbafqmdrhfg.supabase.co"]
    },
    "android": {
      "versionCode": 11,
      "adaptiveIcon": {
        "backgroundColor": "#0A0101",
        // "foregroundImage": "./src/assets/app-icons/android-icon-foreground.png",
        // "backgroundImage": "./src/assets/app-icons/icon-background.png",
        // "monochromeImage": "./src/assets/app-icons/android-icon-monochrome.png",
        "foregroundImage": "./src/assets/app-icons/favicon.png",
        "backgroundImage": "./src/assets/app-icons/favicon.png",
        "monochromeImage": "./src/assets/app-icons/favicon.png"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "predictiveBackGestureEnabled": false,
      "package": "com.haseebahmedrazakhan.tattoola",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "tattoola"
            },
            {
              "scheme": "https",
              "host": "haehelhifvbafqmdrhfg.supabase.co",
              "pathPrefix": "/auth/v1"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/app-icons/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "L'app ha bisogno di accedere alle tue foto per caricare la foto del profilo e i tatuaggi.",
          "cameraPermission": "L'app ha bisogno di accedere alla fotocamera per scattare foto e registrare video."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/app-icons/favicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-font",
      "expo-sqlite",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "624fc895-19a3-4765-9329-1f3ecb47aa61"
      }
    },
    "owner": "figmentadev"
  }
}
