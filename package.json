{
  "name": "tattoola",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env EXPO_ROUTER_APP_ROOT=src/app expo start",
    "dev:setup": "node scripts/dev-setup.js",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "cross-env EXPO_ROUTER_APP_ROOT=src/app expo run:android",
    "ios": "cross-env EXPO_ROUTER_APP_ROOT=src/app expo run:ios",
    "web": "cross-env EXPO_ROUTER_APP_ROOT=src/app expo start --web",
    "lint": "expo lint",
    "build:dev:android": "eas build --platform android --profile development",
    "build:dev:ios": "eas build --platform ios --profile development",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:prod:android": "eas build --platform android --profile production",
    "build:prod:ios": "eas build --platform ios --profile production",
    "update:preview": "eas update --channel preview",
    "update:production": "eas update --channel production"
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.22.0",
    "@expo-google-fonts/montserrat": "^0.4.2",
    "@expo-google-fonts/poppins": "^0.4.1",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@rn-primitives/portal": "^1.3.0",
    "@rn-primitives/slot": "^1.2.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/uuid": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.10",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.7",
    "nativewind": "^4.2.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.3.1",
